<!doctype html>
<html class="no-js" lang="">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title></title>
  <link rel="stylesheet" href="/assets/guitars.css">
  <meta name="description" content="">

</head>

<body>
    <div>
        {% for guitar in guitarList %}
            <div class="product list-item">
                <h3>
                    {{ guitar.productdata.title }} <div class="retailer-fav"><img alt="retailer logo" src="/assets/img/{{ guitar.productdata.retailer }}.png" /></div>
                  </h3>
                  <div class="img-container">
                      <img src={{guitar.displayImage}} alt={name} />
                  </div>
                </div>
        {% endfor %}
    </div>

</body>

</html>